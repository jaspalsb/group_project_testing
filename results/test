    let user = new User();

        user.validate((err) => {
            expect(err.errors.name).to.not.exist;
            expect(err.errors.email).to.exist;
            expect(err.errors.password).to.exist;
            expect(err.errors.stapleTrack).to.not.exist;
            expect(err.errors.zip).to.not.exist;

            done();
